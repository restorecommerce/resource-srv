{
  "name": "@restorecommerce/resource-srv",
  "version": "0.1.0",
  "description": "Restore Commerce Rsource microservice",
  "main": "start.js",
  "author": "Invend GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/resource-srv.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "microservice",
    "resource"
  ],
  "dependencies": {
    "@restorecommerce/chassis-srv": "github:restorecommerce/chassis-srv",
    "@restorecommerce/cluster-service": "^0.1.0",
    "@restorecommerce/kafka-client": "github:restorecommerce/kafka-client",
    "@restorecommerce/protos": "github:restorecommerce/protos",
    "@restorecommerce/resource-base-interface": "github:restorecommerce/resource-base-interface.git",
    "@restorecommerce/service-config": "^0.1.0",
    "co": "^4.6.0",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@restorecommerce/grpc-client": "github:restorecommerce/grpc-client",
    "@types/mocha": "^5.0.0",
    "@types/redis": "^2.8.1",
    "@types/should": "^13.0.0",
    "coveralls": "^3.0.0",
    "istanbul": "^0.4.3",
    "mocha": "^5.1.0",
    "nodemon": "^1.12.1",
    "should": "^13.2.1"
  },
  "scripts": {
    "start": "node start.js",
    "dev": "export NODE_ENV=development; nodemon start.js",
    "pretest": "npm run tsctests",
    "tsctests": "tsc test/*_test.ts --module commonjs -target ES6",
    "test": "npm run lint && npm run mocha",
    "lint": "tslint *.ts",
    "mocha": "export NODE_ENV=test; mocha -R spec ./test/*.js --exit",
    "coveralls": "export NODE_ENV=test; istanbul cover ./node_modules/mocha/bin/_mocha --report lcovonly -- -R spec ./test/*.js --exit",
    "postinstall": "tsc -d"
  },
  "engines": {
    "node": ">= 9.2.0"
  }
}
