{
  "name": "@restorecommerce/resource-srv",
  "version": "0.0.1",
  "description": "Restore Commerce Rsource microservice",
  "main": "lib/start.js",
  "author": "Invend GmbH",
  "repository": {
    "type": "git",
    "url": "https://github.com/restorecommerce/resource-srv.git"
  },
  "license": "MIT",
  "keywords": [
    "restore",
    "commerce",
    "microservice",
    "resource"
  ],
  "dependencies": {
    "@restorecommerce/chassis-srv": "git+https://git@github.com/restorecommerce/chassis-srv.git",
    "@restorecommerce/cluster-service": "^0.1.4",
    "@restorecommerce/grpc-client": "^0.0.4",
    "@restorecommerce/kafka-client": "^0.1.7",
    "@restorecommerce/protos": "git+https://git@github.com/restorecommerce/protos.git",
    "@restorecommerce/resource-base-interface": "git+https://git@github.com/restorecommerce/resource-base-interface.git",
    "@restorecommerce/service-config": "^0.1.6",
    "co": "^4.6.0",
    "lodash": "^4.17.14",
    "redis": "^2.8.0"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^2.9.0",
    "@typescript-eslint/eslint-plugin-tslint": "^2.9.0",
    "@typescript-eslint/parser": "^2.8.0",
    "@types/mocha": "^5.2.7",
    "@types/node": "^12.12.11",
    "@types/redis": "^2.8.13",
    "coveralls": "^3.0.5",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.0",
    "eslint-plugin-prefer-arrow-functions": "^3.0.1",
    "mocha": "^6.2.0",
    "nodemon": "^1.19.2",
    "npm-run-all": "^4.1.5",
    "nyc": "^14.1.1",
    "rimraf": "^3.0.0",
    "should": "^13.2.3",
    "ts-node": "^8.3.0",
    "typescript": "^3.6.3"
  },
  "scripts": {
    "start": "node lib/start.js",
    "dev": "cross-env NODE_ENV=development nodemon --watch './src/**/*.ts' --exec 'ts-node' src/start.ts",
    "pretest": "npm run build && npm run tsctests",
    "tsctests": "tsc -d -p tsconfig.test.json",
    "test": "npm run lint && nyc npm run mocha",
    "lint": "eslint src --ext .ts",
    "mocha": "cross-env NODE_ENV=test mocha -R spec ./test/*.js --exit",
    "mochadebug": "cross-env NODE_ENV=test; mocha -R spec ./test/*.js --full-trace --inspect-brk",
    "coveralls": "nyc report --reporter=text-lcov | coveralls",
    "build:tsc": "tsc -d",
    "build:clean": "rimraf lib",
    "build": "npm-run-all lint build:clean build:tsc",
    "healthcheck": "node lib/healthcheck.js",
    "postinstall": "npm run build:tsc"
  },
  "engines": {
    "node": ">= 9.2.0"
  }
}
