{
  "logger": {
    "console": {
      "handleExceptions": false,
      "level": "crit",
      "colorize": true,
      "prettyPrint": true
    }
  },
  "database": {
    "arango": {
      "provider": "arango",
      "host": "localhost",
      "port": 8529,
      "database": "restore-resource-srv-test",
      "autoCreate": true,
      "collections": [
        "command",
        "organization",
        "contact_point"
      ]
    }
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "offsetStoreInterval": 1000,
    "db-indexes": {
      "db-offsetStore": 0,
      "db-resourcesCounter": 2
    }
  },
  "command-interface": {
    "name": "restore-resource-cis"
  },
  "resources": {
    "restorecommerce": {
      "resourcesProtoPathPrefix": "io/restorecommerce/",
      "resourcesServiceNamePrefix": "io.restorecommerce.",
      "resourcesServiceConfigPrefix": "restore-",
      "resourcesProtoRoot": "node_modules/@restorecommerce/protos/",
      "resources": [
        "command",
        "organization",
        "contact_point"
      ]
    }
  },
  "graph": {
    "graphName": "resource-srv-graph",
    "vertices": {
      "organizations": [
        {
          "edgeName": "org_has_contact_points",
          "from": "id",
          "to": "contact_point_ids",
          "direction": "outbound",
          "toVerticeName": "contact_points"
        }
      ]
    }
  },
  "events": {
    "kafka": {
      "provider": "kafka",
      "groupId": "resource-srv",
      "clientId": "resource-srv",
      "kafkaHost": "localhost:9092",
      "latestOffset": true,
      "restoreCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "resetCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "resetResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "restoreResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "versionCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "versionResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "topics": {
        "command": {
          "topic": "io.restorecommerce.command",
          "events": []
        },
        "contact_points": {
          "topic": "io.restorecommerce.contact_points.resource",
          "events": []
        },
        "organizations": {
          "topic": "io.restorecommerce.organizations.resource",
          "events": []
        }
      }
    },
    "enableCRUDEvents": "true"
  },
  "server": {
    "services": {
      "standard-cfg": {
        "read": {
          "transport": [
            "grpc"
          ]
        },
        "create": {
          "transport": [
            "grpc"
          ]
        },
        "delete": {
          "transport": [
            "grpc"
          ]
        },
        "update": {
          "transport": [
            "grpc"
          ]
        },
        "upsert": {
          "transport": [
            "grpc"
          ]
        }
      },
      "restore-resource-cis": {
        "command": {
          "transport": [
            "grpc"
          ]
        }
      },
      "reflection": {
        "serverReflectionInfo": {
          "transport": [
            "grpc"
          ]
        }
      },
      "graph": {
        "traversal": {
          "transport": [
            "grpc"
          ]
        }
      }
    },
    "transports": [
      {
        "name": "grpc",
        "provider": "grpc",
        "services": {
          "restore-resource-cis": "io.restorecommerce.commandinterface.Service",
          "reflection": "grpc.reflection.v1alpha.ServerReflection",
          "graph": "io.restorecommerce.graph.Service"
        },
        "protos": [
          "io/restorecommerce/commandinterface.proto",
          "grpc/reflection/v1alpha/reflection.proto",
          "io/restorecommerce/graph.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "addr": "localhost:50052"
      }
    ]
  },
  "fieldHandlers": {
    "fieldGenerators": {},
    "bufferFields": {},
    "requiredFields": {}
  },
  "client": {
    "resources": [
      "organization"
    ],
    "default-resource-srv": {
      "transports": {
        "grpc": {
          "service": "",
          "timeout": 3000,
          "protoRoot": "node_modules/@restorecommerce/protos/"
        }
      },
      "loadbalancer": {
        "name": "roundRobin"
      },
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://localhost:50052"
        ]
      },
      "endpoints": {
        "read": {},
        "create": {},
        "delete": {},
        "update": {},
        "upsert": {}
      }
    },
    "commandinterface": {
      "transports": {
        "grpc": {
          "service": "io.restorecommerce.commandinterface.Service",
          "protos": [
            "io/restorecommerce/commandinterface.proto"
          ],
          "protoRoot": "node_modules/@restorecommerce/protos/",
          "timeout": 3000
        }
      },
      "loadbalancer": {
        "name": "roundRobin"
      },
      "publisher": {
        "name": "static",
        "instances": [
          "grpc://localhost:50052"
        ]
      },
      "endpoints": {
        "command": {}
      }
    }
  }
}