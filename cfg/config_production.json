{
  "logger": {
    "elasticsearch": {
      "level": "error",
      "clientOpts": {
        "node": "http://elasticsearch:9200"
      },
      "dataStream": true,
      "source": "resource-srv"
    }
  },
  "redis": {
    "url": "redis://redis:6379"
  },
  "events": {
    "kafka": {
      "kafka": {
        "brokers": [
          "kafka:9092"
        ]
      }
    }
  },
  "database": {
    "arango": {
      "host": "arangodb"
    }
  },
  "server": {
    "transports": [
      {
        "name": "grpc",
        "provider": "grpc",
        "addr": "0.0.0.0:50051"
      }
    ]
  },
  "serviceNames": {
    "cis": "restore-resource-cis"
  },
  "authorization": {
    "cache": {
      "url": "redis://redis:6379"
    }
  },
  "client": {
    "acs-srv": {
      "address": "access-control-srv:50051"
    },
    "user": {
      "address": "identity-srv:50051"
    },
    "graph-srv": {
      "address": "resource-srv:50051"
    }
  }
}