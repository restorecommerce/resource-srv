{
  "database": {
    "arango": {
      "provider": "arango",
      "host": "localhost",
      "port": 8529,
      "database": "restore-resource-srv",
      "autoCreate": true,
      "collections": [
        "command",
        "address",
        "contact_point_type",
        "country",
        "contact_point",
        "credential",
        "locale",
        "location",
        "organization",
        "tax",
        "tax_type",
        "timezone",
        "customer"
      ],
      "customQueries": [
        {
          "path": "filter_ownership.aql",
          "name": "filterByOwnership",
          "type": "filter"
        }
      ]
    }
  },
  "logger": {
    "console": {
      "handleExceptions": false,
      "level": "crit",
      "colorize": true,
      "prettyPrint": true
    },
    "fieldOptions": {
      "omitFields": [
        "custom_arguments"
      ]
    }
  },
  "graph": {
    "graphName": "resource-srv-graph",
    "vertices": {
      "organizations": [
        {
          "edgeName": "org_has_parent_org",
          "from": "id",
          "to": "parent_id",
          "direction": "outbound",
          "toVerticeName": "organizations"
        },
        {
          "edgeName": "org_has_contact_points",
          "from": "id",
          "to": "contact_point_ids",
          "direction": "outbound",
          "toVerticeName": "contact_points"
        }
      ],
      "locations": [
        {
          "edgeName": "loc_is_associated_with_org",
          "from": "id",
          "to": "organization_id",
          "direction": "outbound",
          "toVerticeName": "organizations"
        },
        {
          "edgeName": "loc_has_parent_loc",
          "from": "id",
          "to": "parent_id",
          "direction": "outbound",
          "toVerticeName": "locations"
        },
        {
          "edgeName": "loc_has_address",
          "from": "id",
          "to": "address_id",
          "direction": "outbound",
          "toVerticeName": "addresss"
        }
      ],
      "contact_points": [
        {
          "edgeName": "cp_has_address",
          "from": "id",
          "to": "physical_address_id",
          "direction": "outbound",
          "toVerticeName": "addresss"
        },
        {
          "edgeName": "cp_has_cpt",
          "from": "id",
          "to": "contact_point_type_id",
          "direction": "outbound",
          "toVerticeName": "contact_point_types"
        },
        {
          "edgeName": "cp_has_locale",
          "from": "id",
          "to": "locale_id",
          "direction": "outbound",
          "toVerticeName": "locales"
        }
      ],
      "locales": [
        {}
      ],
      "addresss": [
        {
          "edgeName": "address_belongs_to_country",
          "from": "id",
          "to": "country_id",
          "direction": "outbound",
          "toVerticeName": "countrys"
        }
      ],
      "countrys": [
        {}
      ],
      "tax_types": [
        {}
      ],
      "contact_point_types": [
        {}
      ],
      "credentials": [
        {}
      ],
      "taxs": [
        {
          "edgeName": "tax_taxtype",
          "from": "id",
          "to": "type_id",
          "direction": "outbound",
          "toVerticeName": "tax_types"
        },
        {
          "edgeName": "tax_associated_to_country",
          "from": "id",
          "to": "country_id",
          "direction": "outbound",
          "toVerticeName": "countrys"
        }
      ]
    },
    "edgeDefinitions": [
      {
        "collection": "org_has_parent_org",
        "from": "organizations",
        "to": "organizations"
      },
      {
        "collection": "org_has_contact_points",
        "from": "organizations",
        "to": "contact_points"
      },
      {
        "collection": "loc_is_associated_with_org",
        "from": "locations",
        "to": "organizations"
      },
      {
        "collection": "loc_has_parent_loc",
        "from": "locations",
        "to": "locations"
      },
      {
        "collection": "loc_has_address",
        "from": "locations",
        "to": "addresss"
      },
      {
        "collection": "cp_has_address",
        "from": "contact_points",
        "to": "addresss"
      },
      {
        "collection": "cp_has_cpt",
        "from": "contact_points",
        "to": "contact_point_types"
      },
      {
        "collection": "cp_has_locale",
        "from": "contact_points",
        "to": "locales"
      },
      {
        "collection": "address_belongs_to_country",
        "from": "addresss",
        "to": "countrys"
      },
      {
        "collection": "tax_taxtype",
        "from": "taxs",
        "to": "tax_types"
      },
      {
        "collection": "tax_associated_to_country",
        "from": "taxs",
        "to": "countrys"
      }
    ]
  },
  "redis": {
    "url": "redis://localhost:6379",
    "offsetStoreInterval": 1000,
    "db-indexes": {
      "db-offsetStore": 0,
      "db-resourcesCounter": 2,
      "db-subject": 4
    }
  },
  "resources": {
    "restorecommerce": {
      "resourcesProtoPathPrefix": "io/restorecommerce/",
      "resourcesServiceNamePrefix": "io.restorecommerce.",
      "resourcesServiceConfigPrefix": "restore-",
      "resources": [
        "command",
        "address",
        "contact_point_type",
        "country",
        "contact_point",
        "credential",
        "customer",
        "locale",
        "location",
        "organization",
        "tax",
        "tax_type",
        "timezone"
      ]
    }
  },
  "command-interface": {
    "name": "restore-resource-cis"
  },
  "events": {
    "kafka": {
      "provider": "kafka",
      "groupId": "resource-srv",
      "kafka": {
        "clientId": "resource-srv",
        "brokers": [
          "localhost:29092"
        ]
      },
      "latestOffset": false,
      "restoreCommand": {
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckCommand": {
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "resetCommand": {
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckResponse": {
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "resetResponse": {
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "restoreResponse": {
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "versionCommand": {
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "versionResponse": {
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "configUpdateCommand": {
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "configUpdateResponse": {
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "setApiKeyCommand": {
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "setApiKeyResponse": {
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "flushCacheCommand": {
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "flushCacheResponse": {
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "topics": {
        "command": {
          "topic": "io.restorecommerce.command",
          "events": [
            "restoreCommand",
            "healthCheckCommand",
            "resetCommand",
            "versionCommand",
            "configUpdateCommand",
            "setApiKeyCommand",
            "flushCacheCommand"
          ]
        }
      }
    },
    "enableCRUDEvents": "true"
  },
  "authorization": {
    "service": "acs-srv",
    "enabled": true,
    "enforce": true,
    "urns": {
      "entity": "urn:restorecommerce:acs:names:model:entity",
      "user": "urn:restorecommerce:acs:model:user.User",
      "model": "urn:restorecommerce:acs:model",
      "role": "urn:restorecommerce:acs:names:role",
      "roleScopingEntity": "urn:restorecommerce:acs:names:roleScopingEntity",
      "roleScopingInstance": "urn:restorecommerce:acs:names:roleScopingInstance",
      "unauthenticated_user": "urn:restorecommerce:acs:names:unauthenticated-user",
      "property": "urn:restorecommerce:acs:names:model:property",
      "ownerIndicatoryEntity": "urn:restorecommerce:acs:names:ownerIndicatoryEntity",
      "ownerInstance": "urn:restorecommerce:acs:names:ownerInstance",
      "orgScope": "urn:restorecommerce:acs:model:organization.Organization",
      "subjectID": "urn:oasis:names:tc:xacml:1.0:subject:subject-id",
      "resourceID": "urn:oasis:names:tc:xacml:1.0:resource:resource-id",
      "actionID": "urn:oasis:names:tc:xacml:1.0:action:action-id",
      "action": "urn:restorecommerce:acs:names:action",
      "operation": "urn:restorecommerce:acs:names:operation",
      "execute": "urn:restorecommerce:acs:names:action:execute",
      "permitOverrides": "urn:oasis:names:tc:xacml:3.0:rule-combining-algorithm:permit-overrides",
      "denyOverrides": "urn:oasis:names:tc:xacml:3.0:rule-combining-algorithm:deny-overrides",
      "create": "urn:restorecommerce:acs:names:action:create",
      "read": "urn:restorecommerce:acs:names:action:read",
      "modify": "urn:restorecommerce:acs:names:action:modify",
      "delete": "urn:restorecommerce:acs:names:action:delete",
      "organization": "urn:restorecommerce:acs:model:organization.Organization"
    },
    "cache": {
      "url": "redis://localhost:6379",
      "db-index": 5,
      "ttl": 3600,
      "prefix": "acs:"
    }
  },
  "client": {
    "acs-srv": {
      "address": "localhost:50061"
    },
    "user": {
      "address": "localhost:50051"
    }
  },
  "server": {
    "transports": [
      {
        "name": "grpc",
        "provider": "grpc",
        "addr": "0.0.0.0:50053"
      }
    ]
  },
  "fieldHandlers": {
    "bufferFields": {
      "organization": "data",
      "location": "data",
      "credential": "credentials"
    },
    "requiredFields": {
      "organization": [
        "name"
      ],
      "contact_point_type": [
        "type"
      ],
      "credential": [
        "name"
      ],
      "address": [
        "postcode",
        "country_id",
        "locality",
        "street",
        "region"
      ],
      "country": [
        "name",
        "country_code"
      ],
      "contact_point": [
        "physical_address_id",
        "contact_point_type_id",
        "email",
        "telephone"
      ],
      "location": [
        "name"
      ],
      "tax": [
        "country_id",
        "rate"
      ],
      "tax_type": [
        "type",
        "description"
      ]
    }
  },
  "serviceNames": {
    "cis": "restore-resource-cis"
  },
  "errors": {
    "INVALID_CREDENTIALS": {
      "code": "401",
      "message": "Invalid credentials"
    },
    "USER_NOT_LOGGED_IN": {
      "code": "401",
      "message": "Invalid authentication context, please log in first"
    },
    "ACTION_NOT_ALLOWED": {
      "code": "403",
      "message": "Action not allowed on this resource"
    }
  }
}