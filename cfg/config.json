{
  "logger": {
    "console": {
      "handleExceptions": false,
      "level": "silly",
      "colorize": true,
      "prettyPrint": true
    }
  },
  "database": {
    "arango": {
      "provider": "arango",
      "host": "localhost",
      "port": 8529,
      "database": "restore-resource-srv",
      "autoCreate": true,
      "collections": [
        "command",
        "organization"
      ]
    }
  },
  "graph": {},
  "redis": {
    "host": "localhost",
    "port": 6379,
    "offsetStoreInterval": 1000,
    "db-indexes": {
      "db-offsetStore": 0,
      "db-resourcesCounter": 2
    }
  },
  "resources": {
    "restorecommerce": {
      "resourcesProtoPathPrefix": "io/restorecommerce/",
      "resourcesServiceNamePrefix": "io.restorecommerce.",
      "resourcesServiceConfigPrefix": "restore-",
      "resourcesProtoRoot": "node_modules/@restorecommerce/protos/",
      "resources": [
        "command",
        "organization"
      ]
    }
  },
  "command-interface": {
    "name": "restore-resource-cis"
  },
  "events": {
    "kafka": {
      "provider": "kafka",
      "groupId": "resource-srv",
      "clientId": "resource-srv",
      "kafkaHost": "localhost:9092",
      "latestOffset": false,
      "restoreCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "resetCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "resetResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "restoreResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "versionCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "versionResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "topics": {
        "command": {
          "topic": "io.restorecommerce.command",
          "events": [
            "restoreCommand",
            "healthCheckCommand",
            "resetCommand",
            "versionCommand"
          ]
        }
      }
    },
    "enableCRUDEvents": "true"
  },
  "server": {
    "services": {
      "standard-cfg": {
        "read": {
          "transport": [
            "grpc"
          ]
        },
        "create": {
          "transport": [
            "grpc"
          ]
        },
        "delete": {
          "transport": [
            "grpc"
          ]
        },
        "update": {
          "transport": [
            "grpc"
          ]
        },
        "upsert": {
          "transport": [
            "grpc"
          ]
        }
      },
      "restore-resource-cis": {
        "command": {
          "transport": [
            "grpc"
          ]
        }
      },
      "reflection": {
        "serverReflectionInfo": {
          "transport": [
            "grpc"
          ]
        }
      },
      "graph": {
        "traversal": {
          "transport": [
            "grpc"
          ]
        }
      }
    },
    "transports": [
      {
        "name": "grpc",
        "provider": "grpc",
        "services": {
          "restore-resource-cis": "io.restorecommerce.commandinterface.Service",
          "reflection": "grpc.reflection.v1alpha.ServerReflection",
          "graph": "io.restorecommerce.graph.Service"
        },
        "protos": [
          "io/restorecommerce/commandinterface.proto",
          "grpc/reflection/v1alpha/reflection.proto",
          "io/restorecommerce/graph.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "addr": "localhost:50051"
      }
    ]
  },
  "fieldHandlers": {
    "fieldGenerators": {},
    "bufferFields": {},
    "requiredFields": {}
  }
}
