{
  "database": {
    "arango": {
      "provider": "arango",
      "host": "localhost",
      "port": 8529,
      "database": "restore-resource-srv",
      "autoCreate": true,
      "collections": [
        "command",
        "address",
        "contact_point_type",
        "country",
        "contact_point",
        "credential",
        "locale",
        "location",
        "organization",
        "tax",
        "tax_type",
        "timezone",
        "customer"
      ],
      "customQueries": [
        {
          "path": "filter_ownership.aql",
          "name": "filterByOwnership",
          "type": "filter"
        }
      ]
    }
  },
  "graph": {
    "graphName": "resource-srv-graph",
    "vertices": {
      "organizations": [
        {
          "edgeName": "org_has_parent_org",
          "from": "id",
          "to": "parent_id",
          "direction": "outbound",
          "toVerticeName": "organizations"
        },
        {
          "edgeName": "org_has_contact_points",
          "from": "id",
          "to": "contact_point_ids",
          "direction": "outbound",
          "toVerticeName": "contact_points"
        }
      ],
      "locations": [
        {
          "edgeName": "loc_is_associated_with_org",
          "from": "id",
          "to": "organization_id",
          "direction": "outbound",
          "toVerticeName": "organizations"
        },
        {
          "edgeName": "loc_has_parent_loc",
          "from": "id",
          "to": "parent_id",
          "direction": "outbound",
          "toVerticeName": "locations"
        },
        {
          "edgeName": "loc_has_address",
          "from": "id",
          "to": "address_id",
          "direction": "outbound",
          "toVerticeName": "addresss"
        }
      ],
      "contact_points": [
        {
          "edgeName": "cp_has_address",
          "from": "id",
          "to": "physical_address_id",
          "direction": "outbound",
          "toVerticeName": "addresss"
        },
        {
          "edgeName": "cp_has_cpt",
          "from": "id",
          "to": "contact_point_type_id",
          "direction": "outbound",
          "toVerticeName": "contact_point_types"
        },
        {
          "edgeName": "cp_has_locale",
          "from": "id",
          "to": "locale_id",
          "direction": "outbound",
          "toVerticeName": "locales"
        }
      ],
      "locales": [
        {}
      ],
      "addresss": [
        {
          "edgeName": "address_belongs_to_country",
          "from": "id",
          "to": "country_id",
          "direction": "outbound",
          "toVerticeName": "countrys"
        }
      ],
      "countrys": [
        {}
      ],
      "tax_types": [
        {}
      ],
      "contact_point_types": [
        {}
      ],
      "credentials": [
        {}
      ],
      "taxs": [
        {
          "edgeName": "tax_taxtype",
          "from": "id",
          "to": "type_id",
          "direction": "outbound",
          "toVerticeName": "tax_types"
        },
        {
          "edgeName": "tax_associated_to_country",
          "from": "id",
          "to": "country_id",
          "direction": "outbound",
          "toVerticeName": "countrys"
        }
      ]
    }
  },
  "redis": {
    "host": "localhost",
    "port": 6379,
    "offsetStoreInterval": 1000,
    "db-indexes": {
      "db-offsetStore": 0,
      "db-resourcesCounter": 2
    }
  },
  "resources": {
    "restorecommerce": {
      "resourcesProtoPathPrefix": "io/restorecommerce/",
      "resourcesServiceNamePrefix": "io.restorecommerce.",
      "resourcesServiceConfigPrefix": "restore-",
      "resourcesProtoRoot": "node_modules/@restorecommerce/protos/",
      "resources": [
        "command",
        "address",
        "contact_point_type",
        "country",
        "contact_point",
        "credential",
        "customer",
        "locale",
        "location",
        "organization",
        "tax",
        "tax_type",
        "timezone"
      ]
    }
  },
  "command-interface": {
    "name": "restore-resource-cis"
  },
  "events": {
    "kafka": {
      "provider": "kafka",
      "groupId": "resource-srv",
      "clientId": "resource-srv",
      "kafkaHost": "kafka:9092",
      "latestOffset": false,
      "restoreCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "resetCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "healthCheckResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "resetResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "restoreResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "versionCommand": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandRequest"
      },
      "versionResponse": {
        "protos": [
          "io/restorecommerce/commandinterface.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "messageObject": "io.restorecommerce.commandinterface.CommandResponse"
      },
      "topics": {
        "command": {
          "topic": "io.restorecommerce.command",
          "events": [
            "restoreCommand",
            "healthCheckCommand",
            "resetCommand",
            "versionCommand"
          ]
        }
      }
    },
    "enableCRUDEvents": "true"
  },
  "server": {
    "services": {
      "standard-cfg": {
        "read": {
          "transport": [
            "grpc"
          ]
        },
        "create": {
          "transport": [
            "grpc"
          ]
        },
        "delete": {
          "transport": [
            "grpc"
          ]
        },
        "update": {
          "transport": [
            "grpc"
          ]
        },
        "upsert": {
          "transport": [
            "grpc"
          ]
        }
      },
      "restore-resource-cis": {
        "command": {
          "transport": [
            "grpc"
          ]
        }
      },
      "reflection": {
        "serverReflectionInfo": {
          "transport": [
            "grpc"
          ]
        }
      },
      "graph": {
        "traversal": {
          "transport": [
            "grpc"
          ]
        }
      }
    },
    "transports": [
      {
        "name": "grpc",
        "provider": "grpc",
        "services": {
          "restore-resource-cis": "io.restorecommerce.commandinterface.Service",
          "reflection": "grpc.reflection.v1alpha.ServerReflection",
          "graph": "io.restorecommerce.graph.Service"
        },
        "protos": [
          "io/restorecommerce/commandinterface.proto",
          "grpc/reflection/v1alpha/reflection.proto",
          "io/restorecommerce/graph.proto"
        ],
        "protoRoot": "node_modules/@restorecommerce/protos/",
        "addr": "localhost:50053"
      }
    ]
  },
  "fieldHandlers": {
    "bufferFields": {
      "organization": "data",
      "location": "data",
      "credential": "credentials"
    },
    "requiredFields": {
      "organization": [
        "name"
      ],
      "contact_point_type": [
        "type"
      ],
      "credential": [
        "name"
      ],
      "address": [
        "postcode",
        "country_id",
        "locality",
        "street",
        "region"
      ],
      "country": [
        "name",
        "country_code"
      ],
      "contact_point": [
        "physical_address_id",
        "contact_point_type_id",
        "email",
        "phone_number"
      ],
      "location": [
        "name"
      ],
      "tax": [
        "country_id",
        "rate"
      ],
      "tax_type": [
        "type",
        "description"
      ]
    }
  },
  "serviceNames": {
    "cis": "restore-resource-cis"
  }
}
